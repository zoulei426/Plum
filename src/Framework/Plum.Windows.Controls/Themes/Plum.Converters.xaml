<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:convertors="clr-namespace:Plum.Windows.Convertors;assembly=Plum.Windows.Core">
    <convertors:NotConverter x:Key="NotConverter" />

    <convertors:BoolToFontWeightConverter x:Key="BoolToFontWeightConverter" />
    <convertors:NotBoolToFontWeightConverter x:Key="NotBoolToFontWeightConverter" />
    <convertors:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <convertors:NotBoolToVisibilityConverter x:Key="NotBoolToVisibilityConverter" />
    <convertors:ProgressToVisibilityConverter x:Key="ProgressToVisibilityConverter" />
