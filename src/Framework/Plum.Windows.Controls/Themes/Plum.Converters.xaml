    <convertors:CompletedStatusToIconConverter x:Key="CompletedStatusToIconConverter" />

    <convertors:UriToImageSourceConverter x:Key="UriToImageSourceConverter" />
    <convertors:ByteToImageSourceConverter x:Key="ByteToImageSourceConverter" />

    <convertors:DoubleToRightMarginConverter x:Key="DoubleToRightMarginConverter" />
    <convertors:DoubleToLeftMarginConverter x:Key="DoubleToLeftMarginConverter" />

    <convertors:Add3Converter x:Key="Add3Converter" />
    <convertors:Add5Converter x:Key="Add5Converter" />
    <convertors:Add10Converter x:Key="Add10Converter" />

    <convertors:CloneConverter x:Key="CloneConverter" />
